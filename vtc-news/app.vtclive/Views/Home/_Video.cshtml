<div onclick="closeNav()" class="opacity-background" style=" margin-bottom:100px; margin-top:48px;" data-bind="foreach: videoHomes">
    <div style="width:100%" data-bind="click: $root.loadByVideoByID">
        <div class="img">
            <img src="~/fonts/icon/icon-video.png" class="icon-audio" height=50 width=50 />
            <img class="img-fluid img-content" data-bind="attr: {src: $root.getImage($data, 'Thumbnail')}" />
        </div>
        <div class="container " style="margin-top:10px">
            <h3><strong data-bind="text:Name"></strong>  </h3>
            <p class="size-content"><span style="opacity:0.5" data-bind="text: moment(Schedule()).fromNow()"></span> <span style="color:#a3161d" data-bind="text: Name"></span></p>
            <hr class="hr-bottom" />
        </div>
    </div>
</div>

